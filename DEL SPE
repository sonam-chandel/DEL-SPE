#include<stdio.h>
int main()
{
    int i,a[100],n,pos=1;
    printf("enter size of array");
    scanf("%d",&n);
    printf("enter element");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the position of an element you want delete");
    scanf("%d",&pos);
    for(i=pos-1;i<n;i++)
    {
        a[i]=a[i+1];
    }

    for(i=0;i<n-1;i++){
        printf("array elememt are:%d\n",a[i]);
    }
    return 0;
}
